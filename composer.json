{
	"name": "wp-slab/slab-view",
	"description": "The Slab View component.",
	"type": "wordpress-plugin",
	"license": "MIT",
	"keywords": ["slab", "wordpress"],
	"homepage": "http://www.wp-slab.com/components/view",
	"authors": [{
		"name": "Luke Lanchester",
		"email": "luke@lukelanchester.com",
		"homepage": "http://hybridlogic.co.uk"
	}],
	"autoload": {
		"psr-4": {
			"Slab\\View\\": "src"
		}
	},
	"require": {
		"php": ">=5.5.9",
		"composer/installers": "~1.0.12",
		"wp-slab/slab-core": "dev-master",
		"twig/twig": "^1.23"
	},
	"require-dev": {
		"phpunit/phpunit": "4.8.*",
		"mockery/mockery": "0.9.*"
	},
	"extra": {
		"installer-paths": {
			"vendor/wp-slab/{$name}/": ["type:wordpress-plugin"]
		}
	}
}
